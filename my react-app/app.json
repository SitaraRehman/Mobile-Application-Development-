{
  "expo": {
    "name": "shopping-app",
    "slug": "shopping-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "backgroundColor": "#2c3e50",
      "resizeMode": "contain"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.shoppingapp.app"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#2c3e50"
      },
      "package": "com.shoppingapp.app"
    },
    "web": {
      "bundler": "metro"
    }
  }
}
